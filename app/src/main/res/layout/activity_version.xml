<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="kr.hs.emirim.parksodam.breadclock.VersionActivity">

        <ImageView
        android:id="@+id/imageView4"
        android:layout_width="148dp"
        android:layout_height="206dp"
        app:srcCompat="@drawable/breadclock_icon"
        tools:layout_editor_absoluteY="53dp"
        tools:layout_editor_absoluteX="46dp"
            android:layout_below="@+id/frameLayout"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="31dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="116dp"
        android:layout_height="45dp"
        android:text="현재버전 v6.7.3"
        android:gravity="center"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="179dp"
        android:layout_marginRight="27dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="13dp"
        android:layout_above="@+id/textView8"
        android:layout_alignRight="@+id/button12"
        android:layout_alignEnd="@+id/button12" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="116dp"
        android:layout_height="45dp"
        android:text="최신버전 v6.7.3"
        android:gravity="center"

        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="96dp"
        android:layout_marginBottom="44dp"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />
    <FrameLayout
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="308dp"
        tools:layout_editor_absoluteX="8dp"
        android:id="@+id/frameLayout">

    </FrameLayout>

    <Button
        android:id="@+id/button12"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="현재 최신 버전입니다."
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="46dp"
        android:layout_alignBaseline="@+id/button11"
        android:layout_alignBottom="@+id/button11"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/button11"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:text="업데이트"
        android:gravity="center"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="292dp"
        android:layout_below="@+id/imageView4"
        android:layout_alignRight="@+id/frameLayout"
        android:layout_alignEnd="@+id/frameLayout"
        android:layout_marginTop="32dp" />

</RelativeLayout>
